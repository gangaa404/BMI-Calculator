def bmi(a,b):
       bmi=a/(b**2)
       return bmi

a = float(input("Enter your weight in kilograms:"))
b = float(input("Enter your height in meters:"))

bmi = bmi(a,b)
print("Your BMI is:",bmi)

if bmi < 18.5:
   print("You are underweight.")
elif 18.5 <= bmi < 24.9:
    print("You have a normal weight.")
elif 25 <= bmi < 29.9:
    print("You are overweight.")
else:
     print("You are obese.")